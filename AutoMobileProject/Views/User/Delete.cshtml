@model AutoMobiles.Models.DB.AspNetUsers
@{
    ViewData["Title"] = "Details";
}
<br />
<h2>User Details</h2>
<hr />
<div class="form-border">
    <input type="hidden" asp-for="Id" />
    <fieldset disabled>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="FirstName" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="FirstName" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="LastName" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="LastName" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="Email" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="Email" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="PhoneNumber" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="PhoneNumber" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="Address" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="Address" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="City" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="City" disabled="" class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="PostalCode" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="PostalCode" disabled class="form-control" />
                </div>
            </div>
        </div>
    </fieldset>
    <div class="form-group">
        <input type="submit" value="Delete" class="btn btn-sm btn-danger" />
        <a asp-action="Index" class="btn btn-sm btn-success">Back to List</a>
    </div>
</div>