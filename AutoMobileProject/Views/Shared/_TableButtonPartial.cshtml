@model IndividualButtonPartial

<td style="width: 150px">
    <div class="btn-group" rold="group">
        @Html.Partial("_IndividualButtonPartial",
            new IndividualButtonPartial
            {
                Action = "Edit",
                ButonType = "btn-primary",
                Glyph = "pencil",
                Text = "Edit Button",
                ServiceTypesId = Model.ServiceTypesId,
                CustomerId = Model.CustomerId,
                CarId = Model.CarId
            })

        @Html.Partial("_IndividualButtonPartial",
            new IndividualButtonPartial
            {
                Action = "Details",
                ButonType = "btn-success",
                Glyph = "list",
                Text = "Details Button",
                ServiceTypesId = Model.ServiceTypesId,
                CustomerId = Model.CustomerId,
                CarId = Model.CarId
            })

        @Html.Partial("_IndividualButtonPartial",
            new IndividualButtonPartial
            {
                Action = "Delete",
                ButonType = "btn-danger",
                Glyph = "trash",
                Text = "Delete Button",
                ServiceTypesId = Model.ServiceTypesId,
                CustomerId = Model.CustomerId,
                CarId = Model.CarId
            })
    </div>
</td>