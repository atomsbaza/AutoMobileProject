@model AutoMobileProject.Models.ServiceTypes
@{
    ViewData["Title"] = "Details";
}

<br />
<h2>Details</h2>
<hr />

<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="ServiceName" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="ServiceName" disabled class="form-control">
                </div>
            </div>
        </div>
        <br />
        <div class="form-group">
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-sm btn-primary">Edit</a>
            <a asp-action="Index" class="btn btn-sm btn-success">Back to List</a>
        </div>
    </div>
</div>
